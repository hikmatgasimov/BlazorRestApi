@page "/EmployeeList"
@inherits EmployeeListBase
<h3>EmployeeList</h3>


<table>
    <tr>
        <th>id</th>
        <th>name</th>
        <th>lastname</th>
        <th>email</th>
        <th>dateof</th>
        <th>gender</th>
        <th>dept</th>
    </tr>
</table>
@foreach (var emp in Employees)
{
    <tr>
        <th>@emp.EmployeeId</th>
        <th>@emp.FirstName</th>
        <th>@emp.LastName</th>
        <th>@emp.Email</th>
        <th>@emp.DateOfBrith</th>
        <th>@emp.Gender</th>
        <th>@emp.DepartmentId</th>
       
    </tr>
}